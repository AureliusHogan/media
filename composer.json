{
	"name": "fab/media",
	"type": "typo3-cms-extension",
	"description": "Media management system for TYPO3 CMS.",
	"homepage": "https://github.com/fabarea/media",
	"license": [
		"GPL-2.0+"
	],
	"keywords": [
		"TYPO3 CMS",
		"Media management"
	],
	"support": {
		"issues": "https://github.com/fabarea/media/issues"
	},
	"require": {
		"fab/vidi": ">=2.3.0"
	},
	"autoload": {
		"psr-4": {
			"Fab\\Media\\": "Classes/"
		}
	},
	"replace": {
		"media": "self.version",
		"typo3-ter/media": "self.version"
	}
}
